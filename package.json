{
  "name": "gatsby-transformer-svg",
  "version": "1.0.0",
  "description": "Gatsby transformer plugin for SVG files",
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__"
  },
  "dependencies": {
    "@babel/runtime": "7.9.6",
    "mini-svg-data-uri": "1.1.3",
    "svgo": "1.3.2"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.6",
    "babel-preset-gatsby-package": "0.12.0",
    "cross-env": "7.0.2",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-json": "2.1.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.19.0",
    "babel-eslint": "10.1.0",
    "eslint-plugin-flowtype": "2.50.3",
    "prettier-eslint": "8.8.2",
    "prettier-eslint-cli": "4.7.1"
  },
  "peerDependencies": {
    "gatsby": "^2.0.15"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caki0915/gatsby-transformer-svg"
  },
  "bugs": {
    "url" : "https://github.com/caki0915/gatsby-transformer-svg/issues"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "svg"
  ],
  "files": ["gatsby-node.js"],
  "author": "Carl-Johan Kihl",
  "license": "MIT"
}
